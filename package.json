{
  "name": "@hz-9/redlock-dist",
  "version": "5.0.0-beta.2-2",
  "description": "A derived version of Redlock.",
  "keywords": [
    "nodejs",
    "redlock",
    "lock",
    "redis"
  ],
  "repository": "https://github.com/hz-9/redlock-dist.git",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "browserslist": "node >= 12",
  "ava": {
    "nodeArguments": [
      "--experimental-specifier-resolution=node"
    ]
  },
  "dependencies": {
    "node-abort-controller": "^3.0.1"
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "access": "public"
  }
}